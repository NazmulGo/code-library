      "",
      "ll big_mod(ll a, ll b, ll mod) {",
      "   ll ans = 1;",
      "   while(b) {",
      "   if(b & 1) ans = (ans * a) % mod;",
      "      a = (a * a) % mod;",
      "      b >>= 1;",
      "   }",
      "   return ans;",
      "}",
      "",
      "void prePower() {",
      "   pw[0] = {1, 1};",
      "   for(int i=1; i<N; i++) {",
      "      pw[i].first = (1LL * pw[i-1].first * b1) % M1;",
      "      pw[i].second = (1LL * pw[i-1].second * b2) % M2;",
